    "name": "react-ssr",
    "version": "1.0.0",
    "description": "react ssr ",
    "main": "index.js",
    "author": "monian <zhaoyingqinsummer@gmail.com>",
    "scripts": {
        "dev": "npx ts-node -P ./tsconfig.json ./scripts/dev.ts",
        "dev2": "npx ts-node -P ./tsconfig.json ./scripts/dev2.ts",
        "start": "npx ts-node -P ./tsconfig.json ./scripts/start.ts",
        "server:build": "NODE_ENV=development npx ts-node -P ./tsconfig.json ./scripts/server-build.ts",
        "client:build": "NODE_ENV=production npx ts-node -P ./tsconfig.json ./scripts/client-build.ts",
        "client:start": "NODE_ENV=development npx ts-node -P ./tsconfig.json ./scripts/client-start.ts",
        "server:start": "NODE_ENV=development npx ts-node -P ./tsconfig.json ./scripts/server-start.ts",
        "server": "npx nodemon --watch src/ -e ts,tsx --exec 'ts-node -P ./tsconfig.json ./src/server/src/app.ts'",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "lint": "yarn run lint-eslint",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js {src,scripts}/**/*.{ts,tsx,js}"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:summer789/react-ssr.git"
    },
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
        }
    },
